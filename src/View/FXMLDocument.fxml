<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.FXMLDocumentController">
    <children>
        <Button fx:id="button" layoutX="126" layoutY="90" onAction="#handleButtonAction" text="Click Me!" />
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <Button fx:id="createButton" layoutX="39.0" layoutY="38.0" mnemonicParsing="false" onAction="#createUser" text="Create" />
      <Button fx:id="readButton" layoutX="105.0" layoutY="38.0" mnemonicParsing="false" onAction="#readAll" text="Read" />
      <Button fx:id="updateButton" layoutX="168.0" layoutY="38.0" mnemonicParsing="false" onAction="#updateUser" text="Update" />
      <Button fx:id="deleteButton" layoutX="241.0" layoutY="38.0" mnemonicParsing="false" onAction="#deleteUser" text="Delete" />
      <Button fx:id="readByFullNameButton" layoutX="14.0" layoutY="129.0" mnemonicParsing="false" onAction="#readUserByFullName" text="Read By Full Name" />
      <Button fx:id="readByUsernameAndEmailButton" layoutX="148.0" layoutY="129.0" mnemonicParsing="false" onAction="#readUserByUsernameAndEmail" text="Read By Username &amp; Email" />
    </children>
</AnchorPane>
